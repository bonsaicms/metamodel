{
    "name": "bonsaicms/metamodel",
    "description": "Metamodel for Bonsai CMS",
    "keywords": [
        "laravel",
        "bonsaicms",
        "metamodel"
    ],
    "homepage": "https://github.com/bonsaicms/metamodel",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BonsaiCms\\Metamodel\\": "src",
            "BonsaiCms\\Metamodel\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BonsaiCms\\Metamodel\\MetamodelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
